{
  "name": "fed-app",
  "version": "2.0.0-alpha.4",
  "license": "MIT",
  "homepage": "https://gtdev87.github.io/r_o/",
  "scripts": {
    "start": "concurrently \"npm run js-watch\" \"npm run bsb-watch\"",
    "js-watch": "RAZZLE_APP_ENV=local razzle start",
    "bsb-watch": "bsb -make-world -w",
    "clean": "bsb -clean-world && rm -rf build",
    "build": "bsb -make-world && RAZZLE_APP_ENV=staging razzle build",
    "local-introspection": "rm -rf lib; send-introspection-query http://localhost:5000/graphql",
    "travis": "bsb -make-world && razzle build && cp ./build/public/static/js/bundle.*.js ./build/public/static/js/bundle.js && cp ./build/public/static/css/bundle.*.css ./build/public/static/css/bundle.css && cp ./template/index.html ./build/public/",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "analyze": "RAZZLE_APP_ENV=analyze razzle start",
    "analyze-prod": "RAZZLE_APP_ENV=analyze razzle start:prod"
  },
  "dependencies": {
    "@accounts/client": "0.3.0-beta.30",
    "@accounts/client-password": "0.3.0-beta.30",
    "@accounts/graphql-client": "0.3.0-beta.30",
    "@glennsl/bs-json": "^3.0.0",
    "apollo-boost": "0.1.10",
    "apollo-cache-inmemory": "^1.2.6",
    "apollo-client": "^2.3.7",
    "apollo-link": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-ws": "^1.0.8",
    "emotion": "^9.2.12",
    "express": "4.16.2",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-voyager": "1.0.0-rc.25",
    "http-proxy-middleware": "^0.19.0",
    "prop-types": "15.6.1",
    "query-string": "^6.2.0",
    "random-seed": "^0.3.0",
    "re-debouncer": "^0.0.1",
    "react": "16.2.0",
    "react-apollo": "^2.1.9",
    "react-dom": "16.2.0",
    "react-emotion": "^9.2.12",
    "react-fontawesome": "1.6.1",
    "react-icons": "2.2.7",
    "react-move": "2.7.0",
    "react-sidebar": "^2.3.2",
    "react-sidenav": "2.1.2",
    "react-table": "6.8.0",
    "reactstrap": "6.3.0",
    "reason-apollo": "https://github.com/GTDev87/reason-apollo",
    "reason-react": "0.4.2",
    "recharts": "1.0.0-beta.10",
    "subscriptions-transport-ws": "^0.9.14",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "babel-loader": "7",
    "babel-plugin-emotion": "^9.2.11",
    "babel-plugin-tailwind-components": "^0.5.10",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "bs-platform": "^4.0.6",
    "concurrently": "^3.5.0",
    "graphql_ppx": "^0.2.7",
    "razzle": "2.4.0",
    "tailwindcss": "^0.7.2",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
